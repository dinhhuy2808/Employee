        <form action = '/maintenance' method = 'get'>
            <label>Filtered By </label>
            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <label>First Name :</label>
                    </div>

                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Last Name: </label>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Email: </label>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>Type: </label>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>       </label>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label>       </label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <input  class=" form-control" name = 'fname' id = 'fname' value="<%=data.fnameflt==undefined?"":data.fnameflt%>"  type="text" />
                    </div>

                </div>
                <div class="col-md-3">
                    <div >
                        <input class="form-control" name = 'lname' id = 'lname' value="<%=data.lnameflt==undefined?"":data.lnameflt%>"  type="text" />
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <select class=" form-control" name = 'email' id = 'email'>
                            <option value = ''>------Select-------</option>
                            <%for (i = 0;i<users.length;i++){%>
                            <%if(users[i].email == data.emailflt){%>
                                <option value = '<%=users[i].email%>' selected><%=users[i].email%></option>
                            <%}else{%>
                                <option value = '<%=users[i].email%>' ><%=users[i].email%></option>
                            <%}%>

                            <%}%>
                        </select>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <select class=" form-control" name = 'type' id = 'type'>
                            <option value = '0'selected>------Select-------</option>
                            <option value = '1'>Admin</option>
                            <option value = '2' >Employee</option>
                            <option value = '3'>Customer</option>

                        </select>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class = "row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <input class="btn btn-primary" value = 'Search' type = 'submit'></input>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <a class="btn btn-primary" id = "export" name = "export">Export Excel</a>
                            </div>
                        </div>

                    </div>

                </div>

            </div>

        </form>
        <br/>
        <div class="row">

            <div class="col-md-12">



                <ul class="booking-list">
                    <div class="row">
                        <div class="col-md-2">
                            <h4 class="booking-item-title">First Name</h4>
                        </div>
                        <div class="col-md-2">
                            <h4 class="booking-item-title">Last Name</h4>
                        </div>
                        <div class="col-md-2">
                            <h4 class="booking-item-title">Email</h4>
                        </div>
                        <div class="col-md-2">
                            <h4 class="booking-item-title">Salary</h4>
                        </div>
                        <div class="col-md-2">
                            <h4 class="booking-item-title">Phone Numb</h4>
                        </div>

                    </div>
                    <% for(i = 0;i < users.length;i++){ %>
                    <li>
                        <a class="booking-item" href="/edit-account?email=<%=users[i].email%>">
                            <div class="row">

                                <div class="row">
                                    <div class="col-md-2">
                                        <p class="booking-item-description"><%= users[i].firstname %></p>
                                    </div>
                                    <div class="col-md-2">
                                        <p class="booking-item-description">
                                            <%= users[i].lastname %>

                                        </p>
                                    </div>
                                    <div class="col-md-2">
                                        <p class="booking-item-description"> <%= users[i].email %></p>
                                    </div>

                                    <div class="col-md-2">
                                        <p class="booking-item-description"> <%= users[i].salary %></p>
                                    </div>
                                    <div class="col-md-2">
                                        <p class="booking-item-description" ><%= users[i].phone %></p>
                                    </div>

                                </div>

                            </div>
                        </a>
                    </li>

                    <% } %>


                </ul>

            </div>
        </div>
        <div class="gap"></div>
    </div>



